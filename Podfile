# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Cora' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Cora
  pod 'TinyConstraints'
  pod 'XCoordinator'

  target 'CoraTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
    pod 'Nimble-Snapshots'

  end

end
